= model.team.name
div.currentPlayers
  p Current Players
  each model.team.players as |player|
    ul
      li = player.name
      button.addPlayer{action "removePlayer" player}
        div Remove Me
div.freePlayers
  p Free Players
  each freePlayers as |player|
    ul
      li
        = player.name
        button.addPlayer{action "addPlayer" player}
          div Add Me

